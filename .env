# ROS Configuration
ROS_DOMAIN_ID=42

# Livox MID360 Lidar Configuration
# Network interface connected to the lidar (find with: ip addr show)
LIDAR_INTERFACE=eth0

# Host IP on the lidar subnet (must be on same /24 as lidar)
LIDAR_COMPUTER_IP=192.168.1.5

# Lidar device IP address
LIDAR_IP=192.168.1.116
